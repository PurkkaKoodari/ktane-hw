{
    "compilerOptions": {
        "target": "ES2019",
        "lib": ["DOM", "DOM.Iterable", "ES2019"],
        "skipLibCheck": true,
        "esModuleInterop": true,
        "strict": true,
        "module": "ESNext",
        "moduleResolution": "node",
        "isolatedModules": true,
        "jsx": "react",
        "jsxFactory": "h",
        "jsxFragmentFactory": "Fragment",
    }
}
